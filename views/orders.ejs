<%-include('./partials/header')%>

<div class="container row">
	<%orders.forEach(function(order){ %>
	
	<div class="card col-md-4 m-3" style="width: 18rem;">
		<div class="card-header">
			<%=order.content%>
		</div>
		<div class="card-body">
			<h5 class="card-title">Address: <%=order.content%></h5>
			<h6 class="card-subtitle mb-2 text-muted">Area: <%=order.byUser%></h6>
			<p class="card-text"><%=order.forBusiness%></p>
			<a href="#" class="card-link"><%=order.owner.content%></a>
		</div>
	</div>
	<%});%>
</div>

<%-include('./partials/footer')%>
